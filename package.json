  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^16.0.3",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "cypress": "^12.12.0",
    "typescript": "^5.0.4"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": "cypress/e2e/step_definitions/**/*.steps.{js,ts}",
    "messages": {
      "enabled": true,
      "output": "cucumber-reports/message.ndjson"
    },
    "json": {
      "enabled": true,
      "output": "cucumber-reports/log.json",
      "formater": "cucumber-json-formatter.exe"
    },
    "html": {
      "enabled": true,
      "output": "cucumber-reports/report.html"
    }
  }
